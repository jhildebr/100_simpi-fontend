{
  "appId": "com.simpi.mobile",
  "appName": "SIMPI",
  "webDir": "www",
  "npmClient": "npm",
  "cordova": {
    "preferences": {
      "ScrollEnabled": "false",
      "StatusBarOverlaysWebView": "false",
      "android-minSdkVersion": "19",
