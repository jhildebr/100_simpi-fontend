{
  "name": "simpi",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "export NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "web": "export NODE_OPTIONS=--openssl-legacy-provider && ng serve --project=simpi-frontend-web",
    "mobile": "ionic serve --project=simpi-frontend-mobile",
    "buildandroid": "npm run resourcesandroid && ionic build --project=simpi-frontend-mobile && npx cap sync android && npx cap open android",
    "buildios": "npm run resourcesios && ionic build --project=simpi-frontend-mobile --configuration production --aot && npx cap sync ios && npx cap open ios",
    "buildweb": "ng build --project=simpi-frontend-web --configuration production",
    "buildwebdev": "ng build --project=simpi-frontend-web",
    "resourcesandroid": "cordova-res android --skip-config --copy",
    "resourcesios": "cordova-res ios --skip-config --copy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.13",
