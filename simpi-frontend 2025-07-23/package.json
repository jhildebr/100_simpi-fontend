    "@fortawesome/free-solid-svg-icons": "~5.13.0",
    "@ionic-native/camera": "^5.29.0",
    "@ionic-native/core": "^5.29.0",
    "@ionic-native/deeplinks": "^5.29.0",
    "@ionic-native/file": "^5.32.1",
